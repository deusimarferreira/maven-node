{
  "name": "frontend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "analyze": "webpack-bundle-analyzer target/dist/stats-es5.json",
    "build": "ng build --prod --optimization --aot --buildOptimizer --outputHashing none",
    "build:stats": "npm run build -- --stats-json",
    "e2e": "ng e2e",
    "env-stub": "ng serve --host 0.0.0.0 --proxy-config proxy-stub.conf.json --configuration=stub --poll",
    "json-server": "cp initial.db.json json.server.db.json && node json-server.js",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json -o --poll",
    "start-stub": "run-p json-server env-stub",
    "test": "ng test --watch --browsers=ChromeNoSandbox",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "test-coverage": "rimraf coverage && npm run test -- --watch=false --code-coverage",
    "test-coverage-watch": "rimraf coverage && npm run test -- --code-coverage"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
