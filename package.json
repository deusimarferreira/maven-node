    "normalize.css": "6.0.0",
    "npm-run-all": "4.0.2",
    "protractor": "~5.4.0",
    "rimraf": "2.6.1",
    "ts-node": "~7.0.0",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.4.5",
