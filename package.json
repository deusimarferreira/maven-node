    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@larscom/ngx-translate-module-loader": "^1.3.3",
    "@nebular/date-fns": "4.1.2",
    "@nebular/eva-icons": "4.1.2",
    "@nebular/theme": "4.1.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "4.3.1",
    "chart.js": "^2.8.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "date-fns": ">=2.0.0-alpha.16 <=2.0.0-alpha.27",
    "eva-icons": "^1.1.0",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "keycloak-angular": "^7.0.1",
    "keycloak-js": "^6.0.1",
    "nebular-icons": "1.1.0",
